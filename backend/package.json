{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "concurrently -k \"tsc -w\" \"nodemon -V --watch dist --legacy-watch dist/index.js\"",
    "typeorm": "node ./node_modules/typeorm/cli.js --dataSource dist/data-source.js",
    "migrate:generate": "npm run typeorm migration:generate",
    "migrate:run": "npm run typeorm migration:run",
    "migrate": "npm run build && npm run migrate:run",
    "seed": "npm run build && node dist/seed.js",
    "seed:advanced": "npm run build && node dist/seed-advanced.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
