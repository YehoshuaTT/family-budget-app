    "bcryptjs": "^2.4.3",
    "cliui": "^7.0.4",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0", 
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "string-width": "^4.2.3", 
    "strip-ansi": "^6.0.1",   
    "typeorm": "^0.3.20",    
    "yargs": "^17.7.2"     
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1", 
    "@types/bcryptjs": "^2.4.6",
